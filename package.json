{
  "name": "super-cool-dungeon-game",
  "version": "0.0.1",
  "description": "Super Cool Dungeon Game is a browser javascript game created with Phaser",
  "main": "index.js",
  "repository": {
      "type": "git",
      "url": "https://github.com/FabrizioRizzi/super-cool-dungeon-game-phaser.git"
  }
  "scripts": {
    "start": "parcel src/index.html -p 8000",
    "build": "parcel build src/index.html --out-dir dist"
  },
  "author": "Fabrizio Rizzi <fabrizio@fabriziorizzi.it>",
  "license": "ISC",
  "dependencies": {
    "phaser": "^3.50.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.4.3"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  }
}
